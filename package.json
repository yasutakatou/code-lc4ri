		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.34.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^3.9.4"
	},
	"dependencies": {
		"@types/encoding-japanese": "^1.0.15",
		"date-utils": "^1.2.21",
		"encoding-japanese": "^1.0.30",
		"iconv": "^3.0.0",
		"jschardet": "^2.2.1"
	}
