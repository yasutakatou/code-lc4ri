{
	"name": "code-lc4ri",
	"displayName": "code-lc4ri",
	"description": "LC4RI for VS Code",
	"version": "0.6.0",
	"publisher": "yasutakatou",
	"repository": "https://github.com/yasutakatou/code-lc4ri",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.lc4ri"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.lc4ri",
				"title": "code-lc4ri: LC4RI for VS Code"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
